# Ignore configuration files that may contain sensitive information.
docroot/sites/*/*settings*.php

# Ignore paths that contain generated content.
docroot/files/
docroot/sites/*/files
docroot/sites/*/private

# Ignore default text files
docroot/robots.txt
/CHANGELOG.txt
/COPYRIGHT.txt
/INSTALL*.txt
/LICENSE.txt
/MAINTAINERS.txt
/UPGRADE.txt
/README.txt
docroot/sites/README.txt
docroot/sites/all/modules/README.txt
docroot/sites/all/themes/README.txt

# Ignore everything but the "sites" folder ( for non core developer )
docroot/.htaccess
docroot/web.config
docroot/authorize.php
docroot/cron.php
docroot/index.php
docroot/install.php
docroot/update.php
docroot/xmlrpc.php
/docroot/includes
/docroot/misc
/docroot/modules
/docroot/profiles
/docroot/scripts
/docroot/themes
